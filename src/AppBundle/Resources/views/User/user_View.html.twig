{# EXTENDEMOS LA PLANTILLA 'AppBundle:Layouts:layout.html.twig'...................... #}
{% extends "AppBundle:Layouts:layout.html.twig"%}
{# ......................EXTENDEMOS LA PLANTILLA 'AppBundle:Layouts:layout.html.twig' #}
{% block stylesheets %}
	{# la función parent() carga todo el contenido del bloque anterior #}
	{{ parent() }}
	<!-- switchery Checkbox -->
	<link href="{{ asset ('assets/vendors/switchery/dist/switchery.min.css') }}" rel="stylesheet">
{% endblock %}
{% block content %}
	<!-- page content -->
		<div class="right_col" role="main">
			<div class="clearfix"></div>
			<div class="row">
				<!-- Bloque -->
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<div class="x_panel" >
							<!-- Título del Bloque -->
								<div class="x_title">
									<h3 style="display:inline-block;">Perfil de Usuario</h3>
									<div style="display:block;margin-top:4px;" class="pull-right">
									</div>
									<div class="clearfix"></div>
								</div>
							<!-- /título del Bloque -->
							<!-- Contenido del Bloque -->
								<div class="x_content">
									<div class="col-md-3 col-sm-3 col-xs-12 profile_left">
										<div class="profile_img">
											<div id="crop-avatar">
												{# IMAGEN DE USUARIO SI EXISTIESE O NO UN REGISTRO........... #}
													{% if user.image == null %}
														<img class=" img-responsive center-block avatar-view" src="{{ asset ('assets/images/default_profile.png') }}" alt="default_profile.png" title="Change the avatar">
													{% else %}
														<img class=" img-responsive center-block avatar-view" src="{{ asset ('uploads/users/'~user.id~'/'~user.image) }}" alt="{{user.image}}" title="Change the avatar">
													{% endif %}
												{# ...........IMAGEN DE USUARIO SI EXISTIESE O NO UN REGISTRO #}
											</div>
										</div>
										<div>
											<h3>{{user.name}} {{user.surnames}} ({{user.userName}})</h3>
											<ul class="list-unstyled user_data">
												<li class="m-top-xs">
													<i class="fa fa-external-link user-profile-icon"></i><a href="mailto:{{user.email | lower}}"> {{user.email | lower}}</a><br/>
												</li>
												<li>
													{% if user.gender == "Hombre" %}
														<i class="ionicons ion-male"></i>
														{{user.gender}}<br/>
													{% elseif user.gender == "Mujer" %}
														<i class="ionicons ion-female" ></i>
														{{user.gender}}<br/>
													{% endif %}
												</li>
											</ul>
										</div><br>
										<div class="clearfix"></div>
										<div class="divider-dashed"></div>
										{% if permissionLoggedUser.userPermission == true %}
											<!-- Activa Modal -->
												<div class="pull-left">
													<button class="btn btn-success" data-toggle="modal" data-target="#userEdit" >
														<i class="fa fa-edit m-right-xs"></i>Editar Perfil
													</button>
												</div>
											<!-- /activa Modal -->
										{% endif %}	
										{% if permissionLoggedUser.userPermission == true %}
											<!-- Activa Modal -->
												<div data-toggle="tooltip" data-placement="top" title="Editar Permisos" style="display:inline;">
													<button class="btn btn-success" data-toggle="modal" data-target="#userPermission" >
														<i class="fa fa-chain-broken m-right-xs"></i> 
													</button>
												</div>
											<!-- /activa Modal -->
										{% endif %}
										{% if permissionLoggedUser.userPermission == true %}
											<!-- Google Calendar -->
												<div data-toggle="tooltip" data-placement="top" title="GoogleCalendar" style="display:inline;">
													<a class="btn btn-success"
														href="{{ path("schedule_config", {userId: user.id} ) }}">
														<i class="fa fa-calendar m-right-xs"></i>
													</a>
												</div>		
											<!-- Google Calendar -->
										{% endif %}																				
									</div>
								</div>
							<!-- /contenido del Bloque -->						
						</div>
					</div>
				<!-- /bloque -->				
			</div>
		</div>
	<!-- /page content -->
	<!-- Modal Editar Usuario -->
		{% if permissionLoggedUser.userPermission == true %}
			<div class="modal fade" id="userEdit" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<!-- Modal Header -->
							<div class="modal-header">
								<button type="button" class="close" data-dismiss="modal">
									<span aria-hidden="true">&times;</span>
									<span class="sr-only">Close</span>
								</button>
								<h4 class="modal-title" id="myModalLabel">Editar Permisos Usuario</h4>
							</div>
						<!-- Modal Header -->
						<!-- Modal Body -->
							<div class="modal-body">
								<div class="row control-label form-horizontal form-label-left">
									<!-- Formulario Usuario -->
										{{ form_start(formUserEdit, { 'attr':{'class': 'form-horizontal form-label-left' }})}}
											<div class="col-md-9 col-sm-9 col-xs-12 pull-left">
												<div class="form-group">
													<label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Nombre de Usuario<span class="required">*</span></label>
													<div class="col-md-9 col-sm-9 col-xs-12"> {{form_widget(formUserEdit.userName)}} </div>
												</div>
												<div class="form-group">
													<label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Password<span class="required">*</span></label>
													<div class="col-md-9 col-sm-9 col-xs-12"> {{form_widget(formUserEdit.password)}} </div>
												</div>
												<div class="form-group">
													<label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Email<span class="required">*</span></label>
													<div class="col-md-9 col-sm-9 col-xs-12"> {{form_widget(formUserEdit.email)}} </div>
												</div>
												<div class="form-group">
													<label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Género<span class="required">*</span>
													</label>
													<div class="col-md-9 col-sm-9 col-xs-12"> {{form_widget(formUserEdit.gender)}} </div>
												</div>
												<div class="form-group">
													<label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Imagen<span class="required">*</span>
													</label>
													<div class="col-md-9 col-sm-9 col-xs-12"> {{form_widget(formUserEdit.image)}} </div>
												</div>
											</div>
											<div class="col-md-3 col-sm-3 col-xs-12">
												<div class="profile_img">
													<div id="crop-avatar">
														{# IMAGEN DE USUARIO SI EXISTIESE O NO UN REGISTRO........... #}
														{% if user.image == null %}
															<img class=" img-responsive center-block avatar-view" src="{{ asset ('assets/images/default_profile.png') }}" alt="default_profile.png" title="Change the avatar">
														{% else %}
															<img class=" img-responsive center-block avatar-view" src="{{ asset ('uploads/users/'~user.id~'/'~user.image) }}" alt="{{user.image}}" title="Change the avatar">
														{% endif %}
														{# ...........IMAGEN DE USUARIO SI EXISTIESE O NO UN REGISTRO #}
													</div>
												</div>
											</div>
											<div class="clearfix"></div>
											<div class="divider-dashed"></div>
											<div>
												{{form_widget(formUserEdit.Actualizar)}}
											</div>
										{{ form_end(formUserEdit) }}
									<!-- /formulario Usuario -->	

								</div>
							</div>
					    <!-- Modal Body -->
						{#
							<!-- Modal Footer -->
								<div class="modal-footer"></div>
							<!-- Modal Footer -->
						#}
					</div>
				</div>
			</div>
		{% endif %}
	<!-- /modal Editar Usuario -->
	<!-- Modal Permisos -->
		{% if permissionLoggedUser.userPermission == true %}
