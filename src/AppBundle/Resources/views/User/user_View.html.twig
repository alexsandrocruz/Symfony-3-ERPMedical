																{% else %}
																	{{form_widget (formUserPermission.userPermission, {'attr': {'style' : 'display:none','value' : 0}} ) }}
																{% endif %}
																<label>	 Editar Permisos <sup><i class="fa fa-info-circle m-right-xs" data-toggle="tooltip" data-placement="top" title="{{permissionDescription.userPermission}}"></i></sup></label>
															</div>
														</div>
													<!-- Subsalto Final -->
														<div class="clearfix"></div>
														<div class="divider-dashed"></div>
													<!-- /subsalto Final -->	
												<!-- /usuarios -->
												<!-- Clinic -->
