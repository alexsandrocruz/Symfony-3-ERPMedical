																{% else %}
																	{{form_widget (formUserPermission.userRemove, {'attr': {'style' : 'display:none','value' : 0}} ) }}
																{% endif %}
																<label>	 Eliminar <sup><i class="fa fa-info-circle m-right-xs" data-toggle="tooltip" data-placement="top" title="{{permissionDescription.userRemove}}"></i></sup></label>
															</div>
															<!-- Subsalto -->
																<div class="clearfix"></div><div class="divider-dashed"></div>
															<!-- /subsalto -->
															<div class="col-lg-4 col-md-4 col-sm-6 col-xs-6" style="margin-bottom:5px;">
																{% if userPermission.userDumpView == true %}
																	{{form_widget (formUserPermission.userDumpView, {'attr': {'checked style' : 'display:none','value' : 1}} ) }}
																{% else %}
