															<!-- Subsalto -->
																<div class="clearfix"></div><div class="divider-dashed"></div>
															<!-- /subsalto -->
															<div class="col-lg-4 col-md-4 col-sm-6 col-xs-6" style="margin-bottom:5px;">
																{% if userPermission.medicalHistoryModificationDateEdit == true %}
																	{{form_widget (formUserPermission.medicalHistoryModificationDateEdit, {'attr': {'checked style' : 'display:none','value' : 1}} ) }}
																{% else %}
																	{{form_widget (formUserPermission.medicalHistoryModificationDateEdit, {'attr': {'style' : 'display:none','value' : 0}} ) }}
																{% endif %}
																<label>  Fecha Modificación <sup><i class="fa fa-info-circle m-right-xs" data-toggle="tooltip" data-placement="top" title="{{permissionDescription.medicalHistoryModificationDateEdit}}"></i></sup></label>
															</div>
															<div class="col-lg-4 col-md-4 col-sm-6 col-xs-6" style="margin-bottom:5px;">
																{% if userPermission.medicalHistoryUserModifierEdit == true %}
																	{{form_widget (formUserPermission.medicalHistoryUserModifierEdit, {'attr': {'checked style' : 'display:none','value' : 1}} ) }}
																{% else %}
																	{{form_widget (formUserPermission.medicalHistoryUserModifierEdit, {'attr': {'style' : 'display:none','value' : 0}} ) }}
																{% endif %}
																<label>  Usuario Modificación <sup><i class="fa fa-info-circle m-right-xs" data-toggle="tooltip" data-placement="top" title="{{permissionDescription.medicalHistoryUserModifierEdit}}"></i></sup></label>
															</div>
														</div>	
													<!-- Subsalto Final -->
														<div class="clearfix"></div>
														<div class="divider-dashed"></div>
